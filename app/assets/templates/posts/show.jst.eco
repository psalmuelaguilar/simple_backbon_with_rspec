<div class="col-xs-12 animated fadeIn category-page-posts">
  <br/>
  <br/>
  <p class="meta">
    <a> <%= this.post.author.name %> </a>
    <i class="link-spacer"></i>
    Tags: 
  <% _.each this.post.tag, (tag) =>: %>
  <% console.log tag.id %>

    <a class="tag"> <%= tag.name %> </a>
  <% end %>


  </p>

  <h1> <%= this.post.model.title%> </h1>
  <p> 
    <%= this.post.model.content %>
  </p>
</div>
